( async () => {
  console.log( 'Hello, world!' );
    const response = await fetch( 'https://jsonplaceholder.typicode.com/todos/1' );
    const json = await response.json();
    console.log( json );

    const response1 = await fetch( 'https://my-very-malicious-site.com/todos/1' );
    const json1 = await response.json();
    console.log( json1 );

    const response2 = await fetch( 'https://my-very-very-malicious-site.com/todos/1' );
    const json2 = await response.json();
    console.log( json2 );
} )();
